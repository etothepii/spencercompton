<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <bean id="hamFulCandExt" class="uk.co.epii.conservatives.spencercompton.candidateExtractor.HammersmithAndFulhumExtractor" />

  <bean id="hamFul" class="uk.co.epii.conservatives.spencercompton.Council">
    <property name="name" value="Hammersmith and Fulhum" />
    <property name="url" value="http://www.lbhf.gov.uk/Directory/Council_and_Democracy/Democracy_and_Elections/Electoral_services_voting/" />
    <property name="candidateExtractor" ref="hamFulCandExt" />
  </bean>

  <bean id="fileParser" class="uk.co.epii.conservatives.spencercompton.csvbuilder.FileParser">
    <property name="politicalPartyMap">
      <map>
        <entry key="Conservative Party Candidate" value="65"/>
        <entry key="Green Party" value="103"/>
        <entry key="Labour Party Candidate" value="148"/>
        <entry key="Liberal Democrat" value="153"/>
        <entry key="UK Independence Party (UKIP)" value="364"/>
        <entry key="Trade Unionists and Socialists Against Cuts" value="357"/>
      </map>
    </property>
  </bean>


</beans>